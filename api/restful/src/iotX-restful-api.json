    "definitions": {
        "resUser": {
            "type": "object",
            "description": "用户对象的结构定义",
            "properties": {
                "id": {
                    "type": "string",
                    "description": "用户ID"
                },
                "name": {
                    "type": "string",
                    "description": "用户名称"
                },
                "pwd": {
                    "type": "string",
                    "description": "用户密码"
                },
                "level": {
                    "type": "integer",
                    "description": "用户权限级别"
                },
                "status": {
                    "type": "integer",
                    "description": "用户鉴权状态(已鉴权)(未鉴权)"
                },
                "lastLoginTime": {
                    "type": "string",
                    "description": "用户最后一次鉴权时间"
                },
                "timeout": {
                    "type": "integer",
