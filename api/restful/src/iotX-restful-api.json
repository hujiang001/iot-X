                "key": {
                    "type": "string",
                    "description": "key值，用于鉴权"
                },
                "status": {
                    "type": "integer",
                    "description": "APP鉴权状态(已鉴权)(未鉴权)"
                },
                "lastLoginTime": {
                    "type": "string",
                    "description": "APP最后一次鉴权时间"
